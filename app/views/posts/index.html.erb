<script src="http://d3js.org/queue.v1.min.js"></script>
<% javascript 'timeline.js' %>

<h1>Listing posts</h1>



<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <ol>
    <% @posts.each do |post| %>

      <li>
        <%= link_to post.post_title, post %></br>
        <%= post.post_date %></br>
      </li> </br>


    <% end %>
    </ol>

  </tbody>
</table>

<br>


